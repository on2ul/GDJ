package ex02_casting;

public class Ex03 {

	public static void main(String[] args) {
		
		String strScore = "100";
		String strMoney = "10000000000";
		String strGrade = "4.5";
		
		//아래 변환은 매우 중요하다.
		int score = Integer.parseInt(strScore);
		long money = Long.parseLong(strMoney);
		double grade = Double.parseDouble(strGrade);
		
				
	}

}
